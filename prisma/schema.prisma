generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model todo {
  title       String
  description String
  completed   Boolean @default(false)
  todoId      String  @id
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())
}

model user {
  email    String @unique
  password String
  name     String
  age      Int
  userName String @unique
  userId   String @id
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())
}
